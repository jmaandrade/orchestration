<?xml version="1.0" encoding="ISO-8859-1"?>
<xact:activity xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xact="urn:pt.jma.orchestration.activity">

	<xact:properties>
		<xact:property name="not-too-old-age">70</xact:property>
	</xact:properties>

	<xact:actions/>
	<xact:results />
	<xact:binds>
		<xact:input />
		<xact:output/>
	</xact:binds>

	<xact:events>
		<xact:event name="on-activity-end">
			<xact:bind from="not-too-old-age" scopeFrom="properties" to="age" scopeTo="state" />
		</xact:event>
	</xact:events>

</xact:activity>
