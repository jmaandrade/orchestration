<?xml version="1.0" encoding="iso-8859-1"?>
<xact:activity xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xact="urn:pt.jma.orchestration.activity" parent="teste3Pai">
	
	<xact:properties/>
	<xact:actions/>
	<xact:results/>

		<xact:binds>
			<xact:input />
			<xact:output>
				<xact:bind from="estado" scopeFrom="state" to="estado2" />
			</xact:output>

		</xact:binds>

		<xact:events>

			<xact:event name="final">
				<xact:state name="estado">
