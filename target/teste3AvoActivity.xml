<?xml version="1.0" encoding="iso-8859-1"?>
<xact:activity xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xact="urn:pt.jma.orchestration.activity">
	
	<xact:properties/>
	
	<xact:actions event="inicio"/>

	<xact:results>
		<xact:result name="ok" type="outcome" event="final"/>
	</xact:results>


		<xact:binds>
			<xact:input />
			<xact:output>
				<xact:bind from="estado" scopeFrom="state" to="estado" />
			</xact:output>

		</xact:binds>

		<xact:events>

			<xact:event name="inicio">
				<xact:state name="estado">
					<xact:transition next="execucao"/>
				</xact:state>
			</xact:event>

			<xact:event name="final">
				<xact:state name="estado">
					<xact:transition current="execucao" next="executado"/>
				</xact:state>
			</xact:event>

		</xact:events>

</xact:activity>